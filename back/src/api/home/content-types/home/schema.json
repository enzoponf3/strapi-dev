{
  "kind": "collectionType",
  "collectionName": "homes",
  "info": {
    "singularName": "home",
    "pluralName": "homes",
    "displayName": "Home"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "minLength": 3,
      "required": true,
      "default": "Site Name"
    },
    "Description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks",
      "required": true
    },
    "PresentationImg": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "Phone": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "biginteger",
      "required": true
    },
    "Address": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "Email": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    }
  }
}
